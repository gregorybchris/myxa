{
  "info": {
    "name": "interlet",
    "description": "A blazingly fast webserver",
    "version": {
      "major": 3,
      "minor": 4
    },
    "deps": {
      "flatty": {
        "name": "flatty",
        "version": {
          "major": 2,
          "minor": 0
        }
      }
    }
  },
  "lock": {
    "deps": {
      "flatty": {
        "name": "flatty",
        "version": {
          "major": 2,
          "minor": 0
        }
      }
    }
  },
  "members": {
    "router": {
      "node_type": "mod",
      "name": "router",
      "imports": [
        {
          "path": [
            "flatty"
          ],
          "member_names": [
            "serialize",
            "deserialize"
          ]
        }
      ],
      "members": {
        "serve": {
          "node_type": "func",
          "name": "serve",
          "params": {
            "host": {
              "name": "host",
              "type": "Str"
            },
            "port": {
              "name": "port",
              "type": "Int"
            }
          },
          "return_type": "Null"
        }
      }
    }
  }
}